<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <%= video.categoryName %>
    <ul>
        <% video.categories.forEach(function(item) { %>
          <li><a href="<%= video.siteID %><%= item.categoryID %"><%= item.categoryName %></li>
        <% }); %>
    </ul>
    <% %>
  </body>
</html>
